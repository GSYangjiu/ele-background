    <update id="update" parameterType="Orders">
        update orders
        <set>
            user_id = #{userId},
            shop_id = #{shopId},
            price = #{price},
            status = #{status},
            address_num = #{addressNum},
            create_time = #{createTime},
            update_time = #{updateTime},
